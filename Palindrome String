// my and gfg solution

package GFG_PROBLEM;

public class palindrome_string {
	public static void main (String args[]) {
		String s = "abba";
		char a;
		String a1 = "";
		for(int i=0;i<s.length();i++) {
		a=s.charAt(i);
		a1=a+a1;
		}
		if(s==a1)
			System.out.print("1");
		else
			System.out.print("0");
		
	}
}
